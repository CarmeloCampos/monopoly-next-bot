{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["src/**/*.ts"],
  "rules": {
    "files": "error",
    "dependencies": "error",
    "unlisted": "error",
    "devDependencies": "error",
    "unresolved": "error",
    "exports": "error",
    "types": "error",
    "nsExports": "error",
    "nsTypes": "error",
    "enumMembers": "error",
    "classMembers": "error",
    "duplicates": "error"
  },
  "include": [
    "files",
    "dependencies",
    "unlisted",
    "binaries",
    "unresolved",
    "exports",
    "types",
    "duplicates"
  ],
  "ignore": ["**/*.test.ts", "**/node_modules/**", "**/dist/**"],
  "ignoreDependencies": ["husky"],
  "typescript": true,
  "prettier": true,
  "oxlint": true,
  "bun": true
}
